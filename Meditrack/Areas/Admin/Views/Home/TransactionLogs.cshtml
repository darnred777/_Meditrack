@model IEnumerable<Meditrack.Models.TransactionLogs>

<h2>Transaction Logs</h2>

<table class="table">
    <thead>
        <tr>
            <th>Transaction ID</th>
            <th>Transaction Type</th>
            <th>User ID</th>
            <th>Status ID</th>
            <th>PO Header ID</th>
            <th>PR Header ID</th>
            <th>Product ID</th>
            <th>Quantity</th>
            <th>Transaction Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
            <tr>
                <td>@log.TransactionID</td>
                <td>@log.TransType</td>
                <td>@log.ApplicationUserId</td>
                <td>@log.StatusID</td>
                <td>@log.POHdrID</td>
                <td>@log.PRHdrID</td>
                <td>@log.ProductID</td>
                <td>@log.Quantity</td>
                <td>@log.TransDate.ToString("yyyy-MM-dd HH:mm:ss")</td>
            </tr>
        }
    </tbody>
</table>
